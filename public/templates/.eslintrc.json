{
  "env": {
    "browser": true, 
    "es2021": true,  
    "jest": true     
  },
  "extends": "eslint:recommended", 
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "unicorn" 
  ],
  "rules": {
    "camelcase": "error",
    "unicorn/filename-case": [
      "error",
      {
        "case": "kebabCase"
      }
    ],

    "eqeqeq": "error",

    "no-unused-vars": "warn",

    "semi": ["error", "always"]
  }
}
